@incollection{dimaggio2012facingclass,
	abstract = {Social scientists ordinarily define social class and related concepts in terms of such tangible measures as occupation, income, years of schooling, and home ownership. Humans experience inequality, however, in social interactions, which produce the orderings that our variables index. We learn where we stand by observing how others treat us: deferentially or dismissively; with interest in or indifference to our ideas or desires; with apprehension, reserve, or openness to intimate exchange. Although human status orders are never neatly transitive (Martin 2009), the enactment of status in face-to-face interaction gives us a good idea of our place in the world.Work},
	author = {DiMaggio, Paul},
